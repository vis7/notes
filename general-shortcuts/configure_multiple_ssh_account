# using below file as '~/.ssh/config' to configure multiple account ssh

# Read more about SSH config files: https://linux.die.net/man/5/ssh_config
Host alias
    HostName hostname
    User user

Host vis7
    HostName github.com
    User git
    IdentityFile /home/vishvajeet/.ssh/id_rsa_vis7

Host pg
    HostName ec2-13-233-186-81.ap-south-1.compute.amazonaws.com
    User ubuntu
    IdentityFile /home/vishvajeet/vishvajeet/vprojects/vishvajeet_test_vm.pem
